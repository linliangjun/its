<uniprotocol xmlns="https://its.linliangjun.cn/uniprotocol/0.0.1"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://its.linliangjun.cn/uniprotocol/0.0.1 https://its.linliangjun.cn/uniprotocol/schema/0.0.1">

    <protocol name="JT/T 808 部标协议" version="V2011">
        <mappings message-class-prefix="cn.linliangjun.its.jt808.protocol.message" codec-class-prefix="cn.linliangjun.its.jt808.protocol.codec">
            <mapping message-class="TerminalAuthMessage$TerminalAuthMessage_2011_2013" codec-class="TerminalAuthMessageCodec"/>
        </mappings>
    </protocol>

    <protocol name="JT/T 808 部标协议" version="V2013">
        <mappings message-class-prefix="cn.linliangjun.its.jt808.protocol.message" codec-class-prefix="cn.linliangjun.its.jt808.protocol.codec">
            <mapping message-class="TerminalAuthMessage$TerminalAuthMessage_2011_2013" codec-class="TerminalAuthMessageCodec"/>
        </mappings>
    </protocol>

    <protocol name="JT/T 808 部标协议" version="V2019">
        <mappings message-class-prefix="" codec-class-prefix="">
            <mapping message-class="TerminalAuthMessage.$TerminalAuthMessage_2019" codec-class="TerminalAuthMessageCodec"/>
        </mappings>
    </protocol>
</uniprotocol>
